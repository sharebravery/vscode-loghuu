{
  "publisher": "sharebravery",
  "name": "loghuu",
  "displayName": "Loghuu",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.10.5",
  "description": "VSCode extension for enhanced console logging and code region management.",
  "license": "MIT",
  "homepage": "https://github.com/sharebravery/vscode-loghuu/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/sharebravery/vscode-loghuu"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.85.0"
  },
  "activationEvents": [
    "onCommand:loghuu.consoleLog",
    "onCommand:loghuu.addRegionToSelection"
  ],
  "contributes": {
    "configuration": {
      "title": "Loghuu",
      "properties": {
        "prefix": {
          "type": "string",
          "default": "üöÄ",
          "description": "A custom prefix for print"
        },
        "templateString": {
          "type": "string",
          "default": "console.log('%c${prefix}[${selectedText}]-${currentLineNumber}:', 'color: ${randomColor}', ${selectedText});\n",
          "description": "custom template string(Ëá™ÂÆö‰πâÊ®°ÊùøÂ≠óÁ¨¶‰∏≤)ÔºåprefixÔºåselectedTextÔºåcurrentLineNumberÔºårandomColor‰∏∫ÂèòÈáè"
        }
      }
    },
    "commands": [
      {
        "command": "loghuu.addRegionToSelection",
        "title": "Add Region to Selection",
        "category": "Loghuu"
      },
      {
        "command": "loghuu.consoleLog",
        "title": "Loghuu ConsoleLog",
        "category": "Loghuu"
      },
      {
        "command": "loghuu.consoleWarn",
        "title": "Loghuu ConsoleWarn",
        "category": "Loghuu"
      },
      {
        "command": "loghuu.consoleInfo",
        "title": "Loghuu ConsoleInfo",
        "category": "Loghuu"
      },
      {
        "command": "loghuu.consoleDebug",
        "title": "Loghuu ConsoleDebug",
        "category": "Loghuu"
      },
      {
        "command": "loghuu.consoleDir",
        "title": "Loghuu ConsoleDir",
        "category": "Loghuu"
      }
    ],
    "keybindings": [
      {
        "key": "ctrl+shift+r",
        "command": "loghuu.addRegionToSelection",
        "when": "editorTextFocus && editorHasSelection"
      },
      {
        "command": "loghuu.consoleLog",
        "key": "ctrl+l",
        "mac": "cmd+l",
        "when": "editorTextFocus"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "loghuu.addRegionToSelection",
          "group": "Loghuu",
          "when": "editorTextFocus && editorHasSelection"
        },
        {
          "submenu": "loghuu.commands",
          "group": "Loghuu",
          "title": "Loghuu Commands"
        }
      ],
      "loghuu.commands": [
        {
          "command": "loghuu.consoleLog",
          "group": "Loghuu",
          "title": "Loghuu ConsoleLog",
          "when": "editorTextFocus"
        },
        {
          "command": "loghuu.consoleWarn",
          "group": "Loghuu",
          "title": "Loghuu ConsoleWarn",
          "when": "editorTextFocus"
        },
        {
          "command": "loghuu.consoleInfo",
          "group": "Loghuu",
          "title": "Loghuu ConsoleInfo",
          "when": "editorTextFocus"
        },
        {
          "command": "loghuu.consoleDebug",
          "group": "Loghuu",
          "title": "Loghuu ConsoleDebug",
          "when": "editorTextFocus"
        },
        {
          "command": "loghuu.consoleDir",
          "group": "Loghuu",
          "title": "Loghuu ConsoleDir",
          "when": "editorTextFocus"
        }
      ]
    },
    "submenus": [
      {
        "id": "loghuu.commands",
        "label": "Loghuu Console"
      }
    ]
  },
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && nr publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.1.1",
    "@antfu/ni": "^0.21.9",
    "@types/node": "^20.9.0",
    "@types/vscode": "^1.84.1",
    "@vscode/vsce": "^2.22.0",
    "bumpp": "^9.2.0",
    "eslint": "^8.53.0",
    "esno": "^4.0.0",
    "pnpm": "^8.10.5",
    "rimraf": "^5.0.5",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  }
}
